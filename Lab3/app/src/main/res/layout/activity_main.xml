<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C5CAE9"
    tools:context=".MainActivity">

    <ImageView
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:layout_x="2dp"
        android:layout_y="0dp"
        android:src="@drawable/profile"
        />

    <DatePicker
        android:layout_width="270dp"
        android:layout_height="390dp"
        android:layout_x="130dp"
        android:layout_y="5dp"
        android:background="@color/purple_200"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textSize="25dp"
        android:textColor="#4CAF50"
        android:layout_x="30dp"
        android:layout_y="430dp"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Your Name"
        android:textColor="#4CAF50"
        android:width="200dp"
        android:textSize="25dp"
        android:layout_x="150dp"
        android:layout_y="420dp"

        ></EditText>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textSize="25dp"
        android:textColor="#4CAF50"
        android:layout_x="30dp"
        android:layout_y="490dp"
        ></TextView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Your Email"
        android:textColor="#4CAF50"
        android:width="200dp"
        android:textSize="25dp"
        android:layout_x="150dp"
        android:layout_y="480dp"

        ></EditText>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        android:textSize="25dp"
        android:textColor="#F44336"
        android:layout_x="5dp"
        android:layout_y="540dp"
        ></TextView>

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Male"
        android:textColor="#F44336"
        android:textSize="25dp"
        android:layout_x="110dp"
        android:layout_y="540dp"
        ></RadioButton>

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Female"
        android:textColor="#F44336"
        android:textSize="25dp"
        android:layout_x="210dp"
        android:layout_y="540dp"
        ></RadioButton>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interest"
        android:textSize="25dp"
        android:textColor="#3F51B5"
        android:layout_x="5dp"
        android:layout_y="590dp"
        ></TextView>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sports"
        android:textColor="#3F51B5"
        android:textSize="25dp"
        android:layout_x="120dp"
        android:layout_y="590dp"
        ></CheckBox>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Music"
        android:textColor="#3F51B5"
        android:textSize="25dp"
        android:layout_x="270dp"
        android:layout_y="590dp"
        ></CheckBox>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Literature"
        android:textColor="#3F51B5"
        android:textSize="25dp"
        android:layout_x="120dp"
        android:layout_y="630dp"
        ></CheckBox>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Study"
        android:textColor="#3F51B5"
        android:textSize="25dp"
        android:layout_x="270dp"
        android:layout_y="630dp"
        ></CheckBox>

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="SUBMIT"
        android:textSize="30dp"
        android:textColor="#009688"
        android:background="#FFF176"
        android:layout_x="140dp"
        android:layout_y="677dp"
        ></Button>

</AbsoluteLayout>